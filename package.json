{
  "name": "realtime-leaderboard",
  "version": "0.1.0",
  "description": "Real-time powerful leaderboard with Redis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'lib/**/*.ts' --ignore 'lib/**/*.spec.ts' --exec 'ts-node' lib/index.ts --exec 'jest --detectOpenHandles || true'",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --detectOpenHandles --coverage"
  },
  "keywords": [
    "leaderboard",
    "scoreboard",
    "redis",
    "rank",
    "highscore",
    "gaming",
    "scalable",
    "competition",
    "real-time"
  ],
  "author": "Caner Sezgin <ccanersezgin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/redis": "^2.8.28",
    "ioredis": "^4.23.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "redis": "^3.0.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
